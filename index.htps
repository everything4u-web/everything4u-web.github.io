  <style>
    :root{
      --pink:#d63384;
      --soft-pink:#fff0f5;
      --gold:#c5a059;
      --dark:#2c3e50;
      --white:#ffffff;
      --shadow:0 10px 30px rgba(0,0,0,0.08);
    }

    *{margin:0;padding:0;box-sizing:border-box}

    body{
      font-family:'Poppins','Tiro Devanagari Hindi',sans-serif;
      background:var(--soft-pink);
      color:var(--dark);
      line-height:1.6;
      overflow-x:hidden;
    }

    /* ---------- NAVBAR ---------- */
    nav{
      background:rgba(255,255,255,.98);
      padding:10px 15px;
      position:sticky;
      top:0;
      z-index:1000;
      display:flex;
      align-items:center;
      box-shadow:0 4px 20px rgba(0,0,0,.08);
    }

    .nav-logo-img{
      width:65px;height:65px;
      border-radius:50%;
      border:2px solid var(--gold);
      object-fit:cover;
      margin-right:15px;
      flex-shrink:0;
    }

    .header-title-container{
      display:flex;
      flex-wrap:wrap;
      align-items:center;
      gap:5px;
    }

    .title-col-1{
      font-family:'Cinzel', serif;
      font-size:1.4rem;
      font-weight:800;
      color:var(--pink);
      text-transform:uppercase;
      line-height:1.1;
      letter-spacing: 1px;
    }

    .title-col-2{
      font-family:'Cinzel', serif;
      font-size:1.1rem;
      font-weight:700;
      color:var(--gold);
      text-transform:uppercase;
      line-height:1.1;
      letter-spacing: 1px;
    }

    /* ---------- HERO SECTION (UPDATED FOR front.jpg) ---------- */
    .hero{
      position:relative;
      height:85vh;
      display:flex;
      align-items:flex-end;
      justify-content:center;
      text-align:center;
      color:white;
      padding-bottom:60px;
      overflow: hidden;
      background-color: #333;
    }

    .hero-bg-img {
      position: absolute;
      top: 0;
      left: 0;
      width: 100%;
      height: 100%;
      object-fit: cover;
      z-index: 0;
    }

    .hero-overlay {
      position: absolute;
      top: 0;
      left: 0;
      width: 100%;
      height: 100%;
      background: linear-gradient(to bottom, rgba(0,0,0,0.2), rgba(0,0,0,0.9));
      z-index: 1;
    }

    .hero-content, .ladies-badge-hero {
      position: relative;
      z-index: 2;
    }

    .ladies-badge-hero{
      position:absolute;
      top:20px;
      left:20px;
      background:var(--dark);
      color:white;
      padding:8px 16px;
      border-radius:30px;
      font-size:.8rem;
      font-weight:600;
      letter-spacing:1px;
      display:flex;
      align-items:center;
      gap:8px;
      box-shadow:0 4px 10px rgba(0,0,0,.3);
    }

    .hero-content{
      max-width:90%;
      animation:fadeInUp 1s ease;
    }

    .hero h1{
      font-family:'Playfair Display',serif;
      font-size:2.5rem;
      margin-bottom:0.5rem;
      text-shadow:2px 2px 5px rgba(0,0,0,.6);
    }

    .hero p{
      font-family:'Tiro Devanagari Hindi',serif;
      font-size:1.3rem;
      margin-bottom:2rem;
      text-shadow:1px 1px 3px rgba(0,0,0,.8);
    }

    .btn-main{
      background:var(--pink);
      color:white;
      padding:14px 35px;
      border-radius:50px;
      font-size:1.1rem;
      font-weight:600;
      text-decoration:none;
      display:inline-block;
      box-shadow:0 6px 18px rgba(214,51,132,.6);
      border:2px solid white;
      transition:.3s;
    }

    .btn-main:hover{background:#b01a62;transform:translateY(-2px)}

    /* ---------- SECTIONS ---------- */
    .section-padding{padding:4rem 1rem}

    .section-title{text-align:center;margin-bottom:3rem}
    .section-title h2{font-family:'Playfair Display',serif;font-size:2.3rem;color:var(--dark)}
    .divider{width:70px;height:3px;background:var(--gold);margin:10px auto}

    /* ---------- SERVICES ---------- */
    .services-grid{
      max-width:1200px;margin:auto;
      display:grid;grid-template-columns:repeat(auto-fit,minmax(160px,1fr));
      gap:15px;
    }
    .service-card{
      background:white;border-radius:15px;padding:20px 15px;text-align:center;
      box-shadow:var(--shadow);transition:.3s;border-bottom:3px solid transparent;
    }
    .service-card:hover{transform:translateY(-5px);border-bottom:3px solid var(--gold)}
    .icon-box{font-size:2rem;color:var(--gold);margin-bottom:10px}
    .service-name{font-weight:600;margin-bottom:5px}
    .service-desc{font-size:.9rem;color:#666;font-family:'Tiro Devanagari Hindi',serif}

    /* ---------- GALLERY ---------- */
    .gallery-grid{
        display:grid;grid-template-columns:repeat(auto-fit,minmax(300px,1fr));
        gap:20px;max-width:1200px;margin:0 auto;
    }
    .gallery-item img{
        width:100%;height:250px;object-fit:cover;border-radius:15px;box-shadow:var(--shadow);
    }

    /* ---------- REVIEWS ---------- */
    .reviews-grid{
        display:grid;grid-template-columns:repeat(auto-fit,minmax(300px,1fr));
        gap:20px;max-width:1200px;margin:0 auto;
    }
    .review-card{
        background:white;padding:25px;border-radius:15px;box-shadow:var(--shadow);text-align:center;
    }
    .stars{color:var(--gold);margin-bottom:10px}
    .review-text{font-style:italic;color:#555;font-size:0.95rem;margin-bottom:15px}
    .customer-name{font-weight:700;color:var(--pink)}

    /* ---------- LOCATION & CONTACT ---------- */
    .map-frame{
        max-width:1200px;margin:0 auto;height:350px;
        border-radius:15px;overflow:hidden;box-shadow:var(--shadow);background:#eee;
    }
    
    .contact-actions {
        display: flex;
        justify-content: center;
        gap: 15px;
        margin-top: 20px;
        flex-wrap: wrap;
    }

    .btn-call {
        background: var(--dark);
        color: white;
        padding: 10px 25px;
        border-radius: 30px;
        text-decoration: none;
        display: inline-flex;
        align-items: center;
        gap: 10px;
        font-weight: 600;
        transition: 0.3s;
    }
    .btn-call:hover { background: #000; }

    /* ---------- FOOTER ---------- */
    footer{
      background:var(--dark);color:rgba(255,255,255,.8);text-align:center;
      padding:2rem 1rem;margin-top:4rem;font-size:.9rem;
    }

    /* ---------- WHATSAPP FLOAT ---------- */
    .float-wa{
      position:fixed;right:22px;bottom:22px;width:60px;height:60px;
      background:#25d366;color:white;border-radius:50%;font-size:32px;
      display:flex;align-items:center;justify-content:center;
      box-shadow:0 6px 15px rgba(0,0,0,.35);z-index:9999;text-decoration:none;
    }

    @keyframes fadeInUp{from{opacity:0;transform:translateY(20px)}to{opacity:1;transform:translateY(0)}}

    @media(max-width:768px){
      .hero h1{font-size:2rem}
      .hero p{font-size:1.1rem}
      .nav-logo-img{width:55px;height:55px}
    }
  </style>
</head>

